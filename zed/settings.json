{
  "pane_split_direction_vertical": "right",
  "pane_split_direction_horizontal": "down",
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "version": "2"
  },
  "theme": "Minimal Fedu",
  "indent_guides": {
    "enabled": true
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "use_smartcase_search": true,
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "agent_font_size": 16,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "ui_font_family": "CommitMonossssss",
  "buffer_font_family": "CommitMonossssss",
  "buffer_line_height": {
    "custom": 1.8
  },
  "diagnostics": {
    "include_warnings": true
  },
  "languages": {
    "Vue.js": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
