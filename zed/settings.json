{
  "buffer_font_features": {
    "calt": false
  },
  "pane_split_direction_vertical": "right",
  "pane_split_direction_horizontal": "down",
  "agent": {
    "default_model": {
      "provider": "amazon-bedrock",
      "model": "claude-3-7-sonnet"
    },
    "model_parameters": [],
    "version": "2"
  },
  "language_models": {
    "bedrock": {
      "authentication_method": "named_profile",
      "region": "eu-central-1",
      "profile": "bedrock"
    }
  },
  "current_line_highlight": "none",
  "theme": "Minimal Fedu",
  "indent_guides": {
    "enabled": true
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "use_smartcase_search": true,
  "vim_mode": true,
  "ui_font_size": 17,
  "buffer_font_size": 16,
  "agent_font_size": 17,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "ui_font_family": "CommitMonossssss",
  "buffer_font_family": "CommitMonossssss",
  "ui_font_fallbacks": ["nonicons"],
  "buffer_font_fallbacks": ["nonicons"],
  "buffer_line_height": {
    "custom": 1.5
  },
  "diagnostics": {
    "include_warnings": true
  },
  "active_pane_modifiers": {
    "border_size": 1.75
  },
  "languages": {
    "Vue.js": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
