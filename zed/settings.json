{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "google",
      "model": "gemini-2.0-flash-lite"
    },
    "version": "2"
  },
  "buffer_font_size": 17,
  "ui_font_size": 18,
  "agent_font_size": 18,
  "journal": {
    "path": "~/vault/journal/entries",
    "hour_format": "hour24"
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "buffer_font_features": {
    "calt": false
  },
  "inline_code_actions": false,
  "vim": {
    "use_system_clipboard": "always"
  },
  "use_system_path_prompts": false,
  "use_system_prompts": false,
  "pane_split_direction_vertical": "right",
  "pane_split_direction_horizontal": "down",
  "language_models": {
    "bedrock": {
      "authentication_method": "named_profile",
      "region": "eu-central-1",
      "profile": "bedrock"
    }
  },
  "current_line_highlight": "none",
  "theme": "Minimal Fedu NestJS",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "use_smartcase_search": true,
  "vim_mode": true,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "ui_font_family": "CommitMonoLight",
  "buffer_font_family": "CommitMonoLight",
  "buffer_font_fallbacks": ["nonicons"],
  "diagnostics": {
    "include_warnings": false
  },
  "active_pane_modifiers": {
    "border_size": 1.75
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "completions": {
    "words": "disabled"
  },
  "scrollbar": {
    "git_diff": false
  },
  "languages": {
    "Vue.js": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
